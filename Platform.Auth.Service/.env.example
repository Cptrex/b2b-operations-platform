ASPNETCORE_ENVIRONMENT=Production
ASPNETCORE_URLS=http://+:8080

ServiceJwt__Issuer=auth.service
ServiceJwt__PrivateKeyPath=service_private.pem
ServiceJwt__PublicKeyPath=service_public.pem
ServiceJwt__ExpiresMinutes=5

# Секретные ключи для авторизации сервисов
SERVICE_CREDENTIALS__service.orders=YOUR_ORDERS_SECRET_HERE
SERVICE_CREDENTIALS__service.business=YOUR_BUSINESS_SECRET_HERE
SERVICE_CREDENTIALS__service.notifications=YOUR_NOTIFICATIONS_SECRET_HERE
SERVICE_CREDENTIALS__service.analytics=YOUR_ANALYTICS_SECRET_HERE

# storage for service credentials (optional, for persistent storage)
ConnectionStrings__Postgres=Host=postgres;Port=5432;Database=auth_service_db;Username=auth_user;Password=auth_password
